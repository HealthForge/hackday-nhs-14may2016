tabular-d3
	path
		stroke: steelblue;
		stroke-width: 1;
		fill: none;

	axis
		path, .line
			fill: none;
			stroke: grey;
			stroke-width: 1;
			shape-rendering: crispEdges;

	.brush
		.extent
			stroke: #fff;
			fill-opacity: .125;
			shape-rendering: crispEdges;

	.area
		fill: steelblue;
		clip-path: url(#clip);

	.overlay
		fill: none;
		pointer-events: all;

	.focus
		circle
			fill: none;
			stroke: steelblue;
